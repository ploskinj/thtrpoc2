
<script type="module">

    import { THTR } from '/js/THTR.main.js';
    
    let _set_manager = new THTR.SetManager();
    let _set_avatar = new THTR.SetAvatar();

    $(async function() {

        _set_manager.instantiate();

        
        await _set_avatar.instantiate(THTR.avatar_animation_data.animations.get("male").get(1).get("south"));

        _set_manager.make_entrance(_set_avatar.get_sprite());

    THTR.signal_r_manager.on_tick((timestamp) => {
        console.log("Received tick:", timestamp);
    });

    THTR.signal_r_manager.on_connected(() => {
        console.log("Connected to hub");
    });

    THTR.signal_r_manager.on_disconnected(() => {
        console.log("Disconnected from hub");
    });

    THTR.signal_r_manager.start();

    })

</script>